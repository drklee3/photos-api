{
  "name": "graphql-auth",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
    "start": "node dist/index.ts",
    "clean": "rm -rf dist",
    "build": "yarn -s run clean && yarn -s run generate && tsc",
    "generate": "yarn -s run generate:prisma && yarn -s run generate:nexus && yarn -s run generate:client",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema/schema.ts",
    "generate:client": "graphql-codegen --config codegen.yml",
    "test": "jest"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.42.0",
    "@kenchi/nexus-plugin-prisma": "^0.39.0",
    "@prisma/client": "3.5.0",
    "@types/file-type": "^10.9.1",
    "@types/graphql-upload": "^8.0.7",
    "@types/supertest": "^2.0.11",
    "apollo-server": "3.5.0",
    "argon2": "^0.28.3",
    "axios": "^0.24.0",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.4",
    "exifr": "^7.1.3",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "express-winston": "^4.2.0",
    "fetch-cookie": "^1.0.0",
    "file-type": "^17.0.0",
    "graphql": "15.7.2",
    "graphql-middleware": "6.1.12",
    "graphql-passport": "^0.6.4",
    "graphql-scalars": "1.13.6",
    "graphql-shield": "7.5.0",
    "graphql-upload": "^12.0.0",
    "jsonwebtoken": "8.5.1",
    "nexus": "1.1.0",
    "passport": "^0.5.0",
    "supertest": "^6.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@apollo/client": "^3.5.5",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-graphql-request": "^4.3.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@graphql-codegen/typescript-react-query": "^3.2.1",
    "@types/cors": "^2.8.12",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "8.5.6",
    "@types/node": "^16.11.10",
    "@types/passport": "^1.0.7",
    "@types/tough-cookie": "^4.0.1",
    "@types/uuid": "^8.3.3",
    "jest": "^27.3.1",
    "prisma": "3.5.0",
    "react-query": "^3.33.6",
    "tough-cookie": "^4.0.0",
    "ts-jest": "^27.0.7",
    "ts-node": "10.4.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.5.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
