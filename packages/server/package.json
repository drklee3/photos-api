{
  "name": "@picatch/server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
    "start": "node dist/index.ts",
    "clean": "rm -rf dist",
    "build": "yarn run clean && yarn run generate && tsc",
    "generate": "yarn run generate:prisma && yarn run generate:nexus",
    "generate:prisma": "yarn prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema/schema.ts",
    "test": "yarn build && jest dist/"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.42.0",
    "@aws-sdk/s3-request-presigner": "^3.45.0",
    "@bull-board/express": "^3.8.1",
    "@kenchi/nexus-plugin-prisma": "^0.39.0",
    "@prisma/client": "3.7.0",
    "apollo-server": "3.5.0",
    "argon2": "^0.28.3",
    "axios": "^0.24.0",
    "blurhash": "^1.1.4",
    "bullmq": "^1.54.5",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.4",
    "exif-reader": "^1.0.3",
    "exifr": "^7.1.3",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "fetch-cookie": "^1.0.0",
    "file-type": "^17.0.0",
    "graphql": "15.7.2",
    "graphql-middleware": "6.1.12",
    "graphql-passport": "^0.6.4",
    "graphql-scalars": "1.13.6",
    "graphql-shield": "7.5.0",
    "graphql-upload": "^12.0.0",
    "mime-types": "^2.1.34",
    "nexus": "1.1.0",
    "sharp": "^0.29.3",
    "supertest": "^6.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.5.5",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/cors": "^2.8.12",
    "@types/express-session": "^1.17.4",
    "@types/file-type": "^10.9.1",
    "@types/graphql-upload": "^8.0.7",
    "@types/jest": "^27.0.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "@types/sharp": "^0.29.4",
    "@types/supertest": "^2.0.11",
    "@types/tough-cookie": "^4.0.1",
    "@types/uuid": "^8.3.3",
    "jest": "^27.3.1",
    "prisma": "3.5.0",
    "react": "^17.0.2",
    "react-query": "^3.33.6",
    "tough-cookie": "^4.0.0",
    "ts-node": "10.4.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.5.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
